<div class="row-eq-height">
  <% for icon in @icons : %>
    <div class="thumbnail col-md-3 col-sm-3 col-lg-3">
      <a href ="../icons/<%- icon.url %>" target="_blank" download="../icons/<%- icon.url %>" >
      <img class="" src = "../icons/<%- icon.url %>" alt="<%- icon.name %>">
      </a>
      <h3 class="text-right"><%- icon.name %></h3>

      <p class="text-justify"><%- icon.desc %></p>
    </div>
    <% end %>
</div>
